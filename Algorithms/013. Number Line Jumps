    public static String kangaroo(int x1, int v1, int x2, int v2) {
        if(v1 > v2){
            return (x1 < x2) &&(x2 - x1) % (v1 - v2) == 0 ? "YES" : "NO"; 
        } else if(v1 == v2){
            return x1 == x2 ? "YES" : "NO"; 
        } else {
            return (x2 < x1) && (x1 - x2) % (v2 - v1) == 0 ? "YES" : "NO";
        }
    }
