public static List<Integer> compareTriplets(List<Integer> a, List<Integer> b) {
        int n = 0, m = 0; 
        List<Integer> arr = new ArrayList<>();
        for(int i = 0; i < a.size(); i++){
            if(a.get(i) > b.get(i)){
                n++;
            }
            if(a.get(i) < b.get(i)){
                m++;
            }
        }
        arr.add(n);
        arr.add(m);
        return arr;
